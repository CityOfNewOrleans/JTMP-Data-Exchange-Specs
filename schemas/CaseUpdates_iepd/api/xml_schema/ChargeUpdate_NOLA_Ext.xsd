	<xs:element name="AFISRecievedIndicator" type="niem-xs:boolean">
		<xs:annotation>
			<xs:documentation>The requested data in the Automated Fingerprint Identification System
				(AFIS) at OPSO has been received/ updated. True if an AFIS charge is Received; false
				otherwise.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AFISSentIndicator" type="niem-xs:boolean">
		<xs:annotation>
			<xs:documentation>A data request from AFIS (Finger Printing System at OPSO) has been
				sent. True if an AFIS charge is sent; false otherwise.</xs:documentation>
		</xs:annotation>
	</xs:element>
