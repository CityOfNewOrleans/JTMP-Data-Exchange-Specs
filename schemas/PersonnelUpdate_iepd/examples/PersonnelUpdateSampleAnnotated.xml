<?xml version="1.0" encoding="UTF-8"?>
<nola:PersonnelUpdateExchange xmlns:structures="http://release.niem.gov/niem/structures/5.0/"
    xmlns:ct="http://release.niem.gov/niem/conformanceTargets/3.0/"
    xmlns:appinfo="http://release.niem.gov/niem/appinfo/5.0/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:niem-xs="http://release.niem.gov/niem/proxy/niem-xs/5.0/"
    xmlns:nc="http://release.niem.gov/niem/niem-core/5.0/"
    xmlns:j="http://release.niem.gov/niem/domains/jxdm/7.2/"
    xmlns:scr="http://release.niem.gov/niem/domains/screening/5.2/"
    xmlns:ncic="http://release.niem.gov/niem/codes/ncic/5.2/"
    xmlns:nola="http://mcp.com/nola/v/1.0/"
    xmlns:nola-ext="http://mcp.com/nola/extension/v/1.0/"
    xsi:schemaLocation="http://mcp.com/nola/v/1.0/ file:/C:/Users/gjsadmin/Documents/OxygenXMLEditor/PersonnelUpdateExchange/Personnel_Update_NOLA.xsd">
    <nc:Person>
        <nc:PersonName>
            <!--PrefixTitle-->
            <nc:PersonNamePrefixText>string</nc:PersonNamePrefixText>
            <!--FirstName-->
            <nc:PersonGivenName>string</nc:PersonGivenName>
            <!--MiddleName-->
            <nc:PersonMiddleName>string</nc:PersonMiddleName>
            <!--LastName-->
            <nc:PersonSurName>string</nc:PersonSurName>
            <!--NameSuffix-->
            <nc:PersonNameSuffixText>string</nc:PersonNameSuffixText>
            <!--FullName-->
            <nc:PersonFullName>string</nc:PersonFullName>
        </nc:PersonName>
        <nc:PersonContactInformation>
            <!--EMailAddress-->
            <nc:ContactEmailID>string</nc:ContactEmailID>
            <nc:ContactMailingAddress>
                <!--ContactAddress-->
                <nc:AddressFullText>string</nc:AddressFullText>
                <!--ApartmentNumber-->
                <nc:AddressSecondaryUnitText>string</nc:AddressSecondaryUnitText>
                <!--BoxRouteCode-->
                <nc:AddressDeliveryPointID>string</nc:AddressDeliveryPointID>
                <nc:AddressStreet>
                    <!--StreetDirection-->
                    <nc:StreetFullText>string</nc:StreetFullText>
                    <!--StreetNumber-->
                    <nc:StreetNumberText>string</nc:StreetNumberText>
                    <!--StreetPrefix-->
                    <nc:StreetPredirectional>
                        <nc:StreetDirectionalText>string</nc:StreetDirectionalText>
                    </nc:StreetPredirectional>
                    <!--StreetName-->
                    <nc:StreetName>string</nc:StreetName>
                    <!--StreetSuffix-->
                    <nc:StreetPostdirectional>
                        <nc:StreetDirectionalText>string</nc:StreetDirectionalText>
                    </nc:StreetPostdirectional>
                </nc:AddressStreet>
                <!--AddressCity-->
                <nc:AddressCityName>string</nc:AddressCityName>                
                <nc:AddressState>
                    <!--AddressState-->
                    <nc:StateName>string</nc:StateName>
                    <!--StateCode-->
                    <ncic:StateUSCode>AK</ncic:StateUSCode>
                </nc:AddressState>
                <!--AddressCountry-->
                <nc:AddressCountry>
                    <nc:CountryName>string</nc:CountryName>
                </nc:AddressCountry>
                <!--ZIPCode-->
                <nc:AddressPostalCode>string</nc:AddressPostalCode>
                <!--ZIPCodeExtension-->
                <nc:AddressPostalExtensionCode>string</nc:AddressPostalExtensionCode>
            </nc:ContactMailingAddress>
            <nc:ContactTelephoneNumber>
                <!--FullTelephoneNumber-->
                <nc:FullTelephoneNumber>
                    <nc:TelephoneNumberFullID>string</nc:TelephoneNumberFullID>
                </nc:FullTelephoneNumber>
            </nc:ContactTelephoneNumber>
        </nc:PersonContactInformation>
        <nola-ext:PersonAugmentation>
            <nc:PersonEmploymentAssociation>
                <nc:Employer>
                    <nc:EntityOrganization>
                        <!--AgencyID-->
                        <nc:OrganizationIdentification>
                            <nc:IdentificationID>string</nc:IdentificationID>
                        </nc:OrganizationIdentification>
                        <!--AgencyName-->
                        <nc:OrganizationName>string</nc:OrganizationName>                        
                        <j:OrganizationAugmentation>
                            <!--AgencyORI-->
                            <j:OrganizationORIIdentification>
                                <nc:IdentificationID>string</nc:IdentificationID>
                            </j:OrganizationORIIdentification>
                        </j:OrganizationAugmentation>
                    </nc:EntityOrganization>
                </nc:Employer>
                <!--Officer - RankTypeID-->
                <nc:EmployeeRankText>string</nc:EmployeeRankText>
            </nc:PersonEmploymentAssociation>
            <!--PersonID-->
            <scr:PersonIdentification>
                <nc:IdentificationID>string</nc:IdentificationID>
            </scr:PersonIdentification>
            <!--PersonTypeID-->
            <scr:PersonCategory>
                <scr:PersonCategoryCode>Alien</scr:PersonCategoryCode>
            </scr:PersonCategory>
        </nola-ext:PersonAugmentation>
    </nc:Person>
    <j:EnforcementOfficial>
        <!--OfficerAssignment-->
        <j:EnforcementOfficialAssignmentCategoryText>string</j:EnforcementOfficialAssignmentCategoryText>
        <!--BadgeNumber-->
        <j:EnforcementOfficialBadgeIdentification>
            <nc:IdentificationID>string</nc:IdentificationID>
        </j:EnforcementOfficialBadgeIdentification>
        <!--OfficerType-->
        <j:EnforcementOfficialCategoryText>string</j:EnforcementOfficialCategoryText>
        <j:EnforcementOfficialUnit>
            <!--UnitName-->
            <j:EnforcementUnitName>string</j:EnforcementUnitName>
        </j:EnforcementOfficialUnit>
    </j:EnforcementOfficial>
    <j:JudicialOfficial>        
        <!--JudgeNumber-->
        <j:JudicialOfficialRegistrationIdentification>
            <nc:IdentificationID>string</nc:IdentificationID>
        </j:JudicialOfficialRegistrationIdentification>
        <!--JudgeType-->
        <j:JudicialOfficialCategoryText>string</j:JudicialOfficialCategoryText>
    </j:JudicialOfficial>
    <j:Attorney>
        <!--Attorney - BarNumber-->
        <j:JudicialOfficialBarMembership>
            <j:JudicialOfficialBarIdentification>
                <nc:IdentificationID>string</nc:IdentificationID>
            </j:JudicialOfficialBarIdentification>
        </j:JudicialOfficialBarMembership>
    </j:Attorney>
</nola:PersonnelUpdateExchange>
