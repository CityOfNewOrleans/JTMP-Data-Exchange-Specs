	<xs:import namespace="http://mcp.com/nola/extension/v/1.0/" schemaLocation="./UniformCommitmentOrder_NOLA_Ext.xsd"/>
	<!-- complexType Definitions -->
	<xs:complexType name="UniformCommitmentOrderMessageType">
		<xs:annotation>
			<xs:documentation>A data type containing the data for a Uniform Commitment Order message.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="nola-ext:NOLAExchangeType">
				<xs:sequence>
					<xs:element ref="jCourtOrder" minOccurs="1" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="UniformCommitmentOderMessage" type="nola:UniformCommitmentOrderMessageType">
		<xs:annotation>
			<xs:documentation>A message payload describing a Uniform Commitment Order in NOLA.</xs:documentation>
		</xs:annotation>
	</xs:element>
</xs:schema>
